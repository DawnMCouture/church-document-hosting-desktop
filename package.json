{
  "name": "church-doc-hosting-desktop",
  "version": "1.0.0",
  "description": "What could possibly go wrong",
  "author": "Craig Couture <craigjcouture@gmail.com>",
  "license": "MIT",
  "main": "src/main",
  "scripts": {
    "clean:install": "npm run clean && npm install && npm run lint:fix",
    "clean": "rm -rf package-lock.json node_modules dist",
    "compile:dll": "electron-webpack dll",
    "compile": "electron-webpack",
    "dist:dir": "npm run dist --dir -c.compression=store -c.mac.identity=null",
    "predist": "rm -rf static/files static/temp && mkdir static/files static/temp && echo *\\n!.gitignore > static/files/.gitignore && echo *\\n!.gitignore > static/temp/.gitignore",
    "dist": "npm run compile && electron-builder",
    "lint:fix": "eslint \"src/**\" --fix",
    "postinstall": "npm run rebuild-deps && npm run compile:dll",
    "rebuild-deps": "electron-builder install-app-deps",
    "start": "electron-webpack dev",
    "test": "jest"
  },
  "dependencies": {
    "@rb/resize-observer": "0.0.6",
    "apollo-cache-inmemory": "^1.3.10",
    "apollo-client": "^2.4.6",
    "apollo-link": "^1.2.3",
    "apollo-link-context": "^1.0.9",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "^1.5.5",
    "apollo-link-ws": "^1.0.9",
    "apollo-utilities": "^1.0.25",
    "electron-dl": "^1.12.0",
    "electron-fetch": "^1.3.0",
    "electron-log": "^2.2.17",
    "electron-store": "^2.0.0",
    "express": "^4.16.4",
    "formik": "^1.3.2",
    "fs-extra": "^7.0.1",
    "get-port": "^4.0.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "imagemin": "^6.0.0",
    "imagemin-jpegtran": "^6.0.0",
    "mammoth": "^1.4.7",
    "mkdir-recursive": "^0.4.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-apollo": "^2.3.1",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.12",
    "react-router-dom": "^4.3.1",
    "resize-observer-polyfill": "^1.5.0",
    "rimraf": "^2.6.2",
    "sharp": "^0.21.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "source-map-support": "^0.5.9",
    "styled-components": "^4.1.1",
    "subscriptions-transport-ws": "^0.9.15",
    "through2": "^3.0.0",
    "uuid": "^3.3.2",
    "zip-dir": "^1.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "electron": "^3.0.10",
    "electron-builder": "^20.36.2",
    "electron-webpack": "^2.4.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-stylelint": "^9.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
