    "todo-webpack-plugin": "^1.9.9",
    "webpack": "^4.16.5"
  },
  "build": {
    "appId": "com.electron.church-doc-hosting-desktop",
    "productName": "Church Doc Hosting",
    "mac": {
      "category": "your.app.category.type"
    }
  }
